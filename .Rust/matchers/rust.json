{
    "problemMatcher": [
        {
            "owner": "dglory",
            "pattern": [
                {
                    "regexp": "^(thread\\s+'[^']+'\\s+panicked\\s+at\\s+'[^']+(?:'|$))(?:,\\s+([^/\\\\][^:]+):(\\d+):(\\d+))?",
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "message": 1
                }
            ]
        },
        {
            "owner": "dglory1",
            "pattern": [
                {
                    "regexp": "^(test\\s+\\w+\\s+\\.{3}\\s+FAILED)",
                    "message": 1
                }
            ]
        }
    ]
}
